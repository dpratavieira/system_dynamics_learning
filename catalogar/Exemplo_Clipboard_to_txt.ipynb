   "source": [
    "import pyperclip\n",
    "import os\n",
    "\n",
    "\n",
    "s = pyperclip.paste()\n",
    "\n",
    "# \n",
    "# print(s)\n",
    "\n",
    "# s = s.split('\\r\\n')\n",
    "s = s.split(os.linesep)\n",
    "\n",
    "# print(s, file=open(\"Output1.txt\", \"w\"))\n",
    "\n",
    "with open(\"Output2.txt\", \"w\") as text_file:\n",
    "    for line in s:\n",
    "        text_file.write(line + '\\n')\n",
    "\n",
    "text_file.closed"
   ]
  }
